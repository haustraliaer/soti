{
  "name": "SOTI",
  "version": "0.0.1",
  "description": "Sounds of the Internet",
  "main": "server/index.js",
  "scripts": {
    "start": "node ./Backend",
    "watch": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run pre-render\" \"npm run watch-client\" \"npm run start\"",
    "watch-client": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack.client-watch.js && node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.client-watch.js",
    "pre-render": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack.server.js",
    "build": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack.client-build.js"
  },
  "author": {
    "name": "Msrd"
  },
  "dependencies": {
    "autoprefixer-core": "^5.1.11",
    "axios": "^0.5.4",
    "babel": "5.8.20",
    "babel-core": "5.8.20",
    "babel-loader": "5.3.2",
    "babel-plugin-typecheck": "1.2.0",
    "babel-runtime": "5.8.20",
    "body-parser": "~1.0.1",
    "concurrently": "0.1.1",
    "css-loader": "^0.15.1",
    "express": "~4.0.0",
    "extract-text-webpack-plugin": "^0.8.1",
    "file-loader": "^0.8.1",
    "history": "^1.9.1",
    "immutable": "^3.7.5",
    "json-loader": "0.5.2",
    "lodash": "3.7.0",
    "nedb": "^1.1.4",
    "object-assign": "^4.0.1",
    "piping": "0.2.0",
    "popmotion": "^4.3.1",
    "postcss-calc": "^4.1.0",
    "postcss-color-function": "^1.2.0",
    "postcss-inline-comment": "^1.0.4",
    "postcss-loader": "^0.4.3",
    "postcss-mixins": "^0.3.0",
    "postcss-nested": "^0.3.2",
    "postcss-simple-vars": "^0.3.0",
    "raw-loader": "^0.5.1",
    "react": "0.14.2",
    "react-hot-loader": "1.2.8",
    "react-hotkey": "^0.5.0",
    "react-proxy-loader": "^0.2.1",
    "react-redux": "0.9.0",
    "react-router": "1.0.0-rc1",
    "redux": "1.0.0",
    "redux-thunk": "^1.0.0",
    "source-map-support": "^0.3.2",
    "style-loader": "^0.12.3",
    "webpack": "1.10.5",
    "webpack-dev-server": "1.10.1"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
